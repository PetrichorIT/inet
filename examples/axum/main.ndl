module Client {
    gates {
        port
    }
}

module Server {
    gates {
        port
    }
}

link LAN {
    jitter: 0.0,
    bitrate: 1000000,
    latency: 0.05
}

module Main {
    submodules {
        client: Client,
        server: Server
    }

    connections {
        client/port <-- LAN --> server/port,
    }
}

entry Main;