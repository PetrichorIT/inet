module Emitter {
    gates {
        out
    }
}

module Receiver {
     gates {
        in
    }
}

link LAN {
    jitter: 0.0,
    latency: 0.02,
    bitrate: 1000000,
}

module Main {
    submodules {
        tx: Emitter,
        rx: Receiver
    }

    connections {
        tx/out <-- LAN --> rx/in,
    }
}

entry Main;