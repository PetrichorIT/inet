link Link {
    bitrate: 10000000,
    jitter: 0.0,
    latency: 0.25,
}

module Client {
    gates {
        port,
    } 
}

module Server {
    gates {
        port,
    } 
}

module Main {
    submodules {
        client: Client,
        server: Server
    }

    connections {
        client/port <-- Link --> server/port,
    }
}

entry Main;