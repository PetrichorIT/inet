link LAN {
    jitter: 0.0,
    latency: 0.01,
    bitrate: 10000000,
}

module Node {
    gates {
       port
    }
}

module Switch {
    gates {
        port[5]
    }
}

module Main {
    submodules {
        node[5]: Node,
        switch: Switch
    }

    connections {
        node/port <-- LAN --> switch/port,
    }
}

entry Main;